    }, tiempo);
  }

  function temporizadorIn(target, tiempo) {
    eventoIn = setTimeout(function() {
        $("." + target).fadeIn();
    }, tiempo);            
  }

  function temporizadorOut(target, tiempo) {
    evento = setTimeout(function() {
      $("." + target).fadeOut();
    }, tiempo);
